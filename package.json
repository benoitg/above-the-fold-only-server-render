    "@walmart/electrode-docgen": "^1",
    "@walmart/lithe-extras": "^2.8.0",
    "builder": "^2.10.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8"
  },
  "engines": {
    "node": "^4.2.6",
    "npm": "^3.5.3"
  }
